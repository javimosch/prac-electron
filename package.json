{
    "name": "prak-photo",
    "version": "2.11.0",
    "main": "dist/electron/main/index.js",
    "description": "Advanced file deduplication software",
    "author": "arancibiajav@gmail.com",
    "email": "arancibiajav@gmail.com",
    "license": "PROPIETARY",
    "homepage": "https://prak.lunardog.com/",
    "private": true,
    "scripts": {
        "dev": "VITE_DEV_SERVER_URL=localhost vite",
        "build": "cross-env NODE_ENV=production vue-tsc --noEmit && vite build && electron-builder",
        "build:docker": "npm ci && npx modclean --run && cross-env NODE_ENV=production vite build && electron-builder",
        "release": "git push origin master && git push gh master:main",
        "release:mayor": "npx json-bump package.json --mayor && git add package.json && git commit -m 'Bump version' && git push origin master && git push gh master:main",
        "release:minor": "npx json-bump package.json --minor && git add package.json && git commit -m 'Bump version' && git push origin master && git push gh master:main",
        "release:patch": "npx json-bump package.json --patch && git add package.json && git commit -m 'Bump version' && git push origin master && git push gh master:main",
        "bump:mayor": "npx json-bump package.json --mayor && git add package.json && git commit -m 'Bump version' && git push origin master",
        "bump:minor": "npx json-bump package.json --minor && git add package.json && git commit -m 'Bump version' && git push origin master",
        "bump:patch": "npx json-bump package.json --patch && git add package.json && git commit -m 'Bump version' && git push origin master",
        "test": "jest",
        "prepare": "husky install",
        "icon": "npx eib  -i ./src/assets/icons/1024x1024.png -o ./resources"
    },
    "build": {
        "appId": "com.lunardog.prakelectron",
        "productName": "Prak Photo",
        "asar": true,
        "compression": "store",
        "win": {
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64"
                    ]
                }
            ]
        },
        "mac": {
            "category": "public.app-category.utilities",
            "entitlements": "entitlements.mac.plist",
            "target": [
                {
                    "target": "dmg"
                },
                {
                    "target": "pkg"
                }
            ]
        },
        "linux": {
            "target": [
                {
                    "target": "deb"
                },
                {
                    "target": "AppImage"
                }
            ],
            "category": "Utility",
            "desktop": {
                "Name": "Prak Photo",
                "Type": "Application",
                "Categories": "System Utilities"
            },
            "executableName": "prakphoto",
            "maintainer": "arancibiajavas@gmail.com"
        },
        "directories": {
            "output": "release",
            "buildResources": "resources"
        },
        "files": [
            "dist/**/*",
            "resources/**/*",
            "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
            "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
            "!**/node_modules/*.d.ts",
            "!**/node_modules/.bin",
            "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
            "!.editorconfig",
            "!**/._*",
            "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
            "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
            "!**/{appveyor.yml,.travis.yml,circle.yml}",
            "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
        ]
    },
    "engines": {
        "node": ">=14.17.0"
    },
    "devDependencies": {
        "@hunlongyu/electron-icon-builder": "^0.1.5",
        "@types/jest": "^29.4.2",
        "@vicons/material": "^0.12.0",
        "@vicons/utils": "^0.1.4",
        "@vitejs/plugin-vue": "^2.3.4",
        "@vue/devtools": "^6.5.0",
        "cross-env": "^7.0.3",
        "electron": "^21.0.1",
        "electron-builder": "^23.1.0",
        "husky": "^8.0.0",
        "jest": "^29.5.0",
        "modclean": "^3.0.0-beta.1",
        "pinia": "^2.0.28",
        "pug": "^3.0.2",
        "sass": "^1.55.0",
        "ts-jest": "^29.0.5",
        "typescript": "^4.8.4",
        "unplugin-auto-import": "^0.15.1",
        "unplugin-vue-components": "^0.22.0",
        "vfonts": "^0.0.3",
        "vite": "^2.9.13",
        "vite-plugin-electron": "^0.9.3",
        "vite-plugin-tsc-watch": "^2.0.1",
        "vue": "^3.2.40",
        "vue-tsc": "^0.39.4",
        "vue3-simple-svg": "^1.0.0"
    },
    "env": {
        "VITE_DEV_SERVER_HOST": "127.0.0.1",
        "VITE_DEV_SERVER_PORT": 3344
    },
    "keywords": [
        "electron",
        "rollup",
        "vite",
        "vue3",
        "vue"
    ],
    "dependencies": {
        "@directus/sdk": "^10.3.1",
        "electron-cfg": "^1.2.7",
        "electron-log": "^4.4.8",
        "electron-store": "^8.1.0",
        "empty-dir": "^3.0.0",
        "exif-parser": "^0.1.12",
        "md5-file": "^5.0.0",
        "mime-types": "^2.1.35",
        "moment-timezone": "^0.5.37",
        "node-readfiles": "^0.3.1",
        "promise-all-sequential": "^1.0.0",
        "promise-sequential": "^1.1.1",
        "recursive-readdir-async": "^1.2.1",
        "sander": "^0.6.0",
        "shortid": "^2.2.16",
        "ts-node": "^10.9.1",
        "vue-router": "^4.1.6",
        "vue3-sortablejs": "^1.0.6",
        "xhr2": "^0.2.1",
        "zod": "^3.21.4"
    }
}
